{
  "name": "vizabi-reactive",
  "version": "1.3.0",
  "description": "Vizabi core for interactive animated charts. This version uses mobx for state and event mgmt",
  "main": "dist/Vizabi.js",
  "files": [
    "dist/Vizabi.js",
    "dist/Vizabi.js.map",
    "dist/Dataframe.js",
    "dist/Dataframe.js.map",
    "src"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npx rollup -c",
    "start": "webpack-dev-server -d --open",
    "rollup": "rollup -c",
    "test": "jest",
    "debug": "node node_modules/.bin/jest ./*/dataConfig.test.js --runInBand --watch --no-cache"
  },
  "author": "Jasper Heeffer and others at Gapminder Foundation",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/vizabi/vizabi-reactive"
  },
  "bugs": {
    "url": "https://github.com/vizabi/vizabi-reactive/issues"
  },
  "homepage": "http://vizabi.org",
  "dependencies": {
    "mobx": "^5.15.7"
  },
  "peerDependencies": {
    "mobx": "^5.15.7",
    "d3": "^6.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.12.1",
    "install-peers": "^1.0.3",
    "jest": "^24.7.1",
    "jest-cli": "^26.6.3",
    "mobx-utils": "^5.5.2",
    "mobx-formatters": "^1.0.2",
    "webpack": "^4.29.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "rollup": "^1.25.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "vizabi-ddfcsv-reader": "^4.0.1"
  },
  "serve": {
    "content": "./dist",
    "hot": false
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/jest/setup.js"
    ]
  }
}
